    vdiff *= 0.8;
    hdiff *= 0.8;
}

player.x += hdiff;
player.y += vdiff;


if (Space_Pressed) {
    if ( player.recharge &gt;= 360){
        audio_play_sound( snd_wave, 1, false );
        instance_create( player.x, player.y, obj_wave );
        player.recharge = 1;
    }
}
